    console.log(` ðŸ›‘ðŸ›‘ðŸ›‘  Assertion Failed:  ${actual} !== ${expected}`);
  }
  // TEST CODE  
}

assertEqual("Lighthouse Labs", "Bootcamp");
assertEqual(1, 1);
