const eqArray = function (array1, array2) {
  for (let i = 0; i < array1.length; i++) {
    for (let k = 0; k < array2.length; k++) {
      if (array1[i] === array2[k]) {
        return true;
        
        console.log("array 1= array 2");
      }

    }
  }
  return false;
}


